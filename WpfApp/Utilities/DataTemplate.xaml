<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:wpfapp.ViewModel"
                    xmlns:view="clr-namespace:wpfapp.View">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DashboardViewModel}">
        <view:Dashboard />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FileViewModel}">
        <view:fileview />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TreeViewModel}">
        <view:Tree />
    </DataTemplate>


    <DataTemplate DataType="{x:Type vm:SettitngsViewModel}">
        <view:SettingsView />
    </DataTemplate>

</ResourceDictionary>